{
  "name": "piccolo",
  "version": "0.0.0",
  "description": "A thin NodeJS wrapper around Denon's Hi-Fi Remote",
  "main": "index.js",
  "scripts": {
    "build": "del './build' && babel ./lib -d ./build --ignore '**/*.spec.js'",
    "test": "tape -r ./test/setup.js 'lib/**/*.spec.js' | tap-spec --color",
    "test:watch": "tape-watch -w -r ../test/setup.js 'lib/**/*.spec.js' -p tap-spec --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adambrgmn/piccolo.git"
  },
  "author": "Adam Bergman <adam@fransvilhelm.com> (http://fransvilhelm.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adambrgmn/piccolo/issues"
  },
  "homepage": "https://github.com/adambrgmn/piccolo#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-config-adambrgmn": "^0.1.4",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "blue-tape": "^1.0.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.7.0",
    "eslint-config-adambrgmn": "^0.3.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "tape-watch": "^2.2.3"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "cross-spawn": "^4.0.2",
    "evilscan": "^1.0.2",
    "promise-poller": "^1.5.1",
    "ramda": "^0.22.1",
    "stampit": "^3.0.2"
  }
}
